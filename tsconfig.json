{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "esnext",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "allowJs": true,
    "jsx": "react",
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true
  },
  "include": ["lib/**/*"]
}
